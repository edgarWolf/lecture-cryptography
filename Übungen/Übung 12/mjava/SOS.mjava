boolean main() {
    println("Berechnen des SOS Problems...");

    int[] a = new int[] {1, 2, 3, 5, 7, 12};
    int b = 6; 

    boolean sumFound = false;
    int sum = 0;
    for (int i = 0; i < 6; i++) {
        split sum = sum + a[i]; and sum = sum;
        if (sum == b) {
            sumFound = true;
        }
    }

    if (sumFound) {
        println("Summe fuer b=" + b + " gefunden.");
    }
    return sumFound;
}